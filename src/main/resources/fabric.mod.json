{
	"schemaVersion": 1,
	"id": "togglenametags",
	"version": "${version}",
	"name": "ToggleNametags",
	"description": "A mod that allows you to toggle your nametags.",
	"authors": [
		"Andreas <a@ndy.sh>"
	],
	"contact": {
		"homepage": "https://github.com/Greek/ToggleNametags",
		"sources": "https://github.com/Greek/ToggleNametags"
	},
	"license": "CC0-1.0",
	"icon": "assets/togglenametags/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"sh.ndy.ToggleNametags"
		],
		"client": [
			"sh.ndy.ToggleNametagsClient"
		]
	},
	"mixins": [
		"togglenametags.mixins.json",
		{
			"config": "togglenametags.client.mixins.json",
			"environment": "client"
